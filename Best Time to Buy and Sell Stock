/**
 * Created by Loki on 2016/10/16.
 */
 
 public class Solution {
    public int maxProfit(int[] prices) {
        if(prices.length < 2) return 0;
        int profit = 0;
        int index = 0;
        for(int i = 1; i < prices.length; i++){
            int temp = prices[i] - prices[index];
            if(profit < temp) profit = temp;
            if(temp <= 0) index = i;
        }
        return profit;
    }
}
